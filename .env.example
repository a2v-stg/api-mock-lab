# Mock-Lab Environment Configuration
# Copy this file to .env and update the values

# Database Configuration (PostgreSQL)
POSTGRES_DB=mocklab
POSTGRES_USER=mocklab
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_PORT=5432

# Application Configuration
SECRET_KEY=change-this-to-a-random-secret-key-in-production
DATABASE_URL=postgresql://mocklab:your_secure_password_here@db:5432/mocklab

# Service Ports
BACKEND_PORT=8001
FRONTEND_PORT=3000

# For development with SQLite (alternative to PostgreSQL)
# DATABASE_URL=sqlite:///./mocker.db

# Production Settings (optional)
# ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
# LOG_LEVEL=info
# WORKERS=4
